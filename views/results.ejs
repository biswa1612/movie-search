<h1>
	Results page
</h1>
<ul>
	<% data["Search"].forEach(function(movie) { %>
	<li>
		<div class="container">
			<div>
			<strong><%= movie["Title"] %></strong> - <%= movie["Year"] %> - <a href="https://www.imdb.com/title/<%= movie["imdbID"] %>/">imdb info</a>
		</div>
		<div class="thumbnail">
			<img src="<%= movie["Poster"] %>" width="150" height="150">
		</div>
		</div>
		
		
		
	</li>
	<% }) %>
</ul>
<a href="/">Search Again!!</a>